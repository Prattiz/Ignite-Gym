{
  "expo": {
    "name": "ignitegym",
    "slug": "ignitegym",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": ".src/assets/icon.svg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ".src/assets/icon.svg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "plugins": [
      "expo-font"
    ]
  }
}
